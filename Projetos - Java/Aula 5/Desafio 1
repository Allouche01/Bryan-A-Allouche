public class Desafio_1 {

    public static void main(String[] args) {

        int qduc;

        qduc = (int) Float.parseFloat(JOptionPane.showInputDialog(null, "Qual a quantidade de dias da sua última compra ? "));

        if (qduc <= 30) {
            JOptionPane.showMessageDialog(null,
                    "Você possui desconto de 10%",
                    "Dados",
                    JOptionPane.INFORMATION_MESSAGE);
        } else if (qduc <= 50) {
            JOptionPane.showMessageDialog(null,
                    "Você possui desconto de 20%",
                    "Dados",
                    JOptionPane.INFORMATION_MESSAGE);
        } else if (qduc <= 100) {
            JOptionPane.showMessageDialog(null,
                    "Você possui desconto de 30%",
                    "Dados",
                    JOptionPane.INFORMATION_MESSAGE);
        } else if (qduc <= 120) {
            JOptionPane.showMessageDialog(null,
                    "Você possui desconto de 40%",
                    "Dados",
                    JOptionPane.INFORMATION_MESSAGE);
        }
    }
}
